<form script="table_data_bar.js" stylesheet="table_data_bar.css">
  <label>IMS transaction CPU and elapsed time breakdown (from OMEGAMON ATF)</label>
  <search id="base_timechart">
    <query>`fuw_search_start_ims` sourcetype=atf-aes $imsid$ $tran$ $response_min$ $response_max$ | timechart count avg(aescpu) avg(aesxsrsp) avg(aescpui) avg(aeselti) avg(aesxsacp) avg(aescpub) avg(aeseltb) avg(aescpud) avg(aescpus) avg(aescpuq) avg(aeseltq) avg(aeseltq) avg(aesxsiqt) avg(aeselaps) avg(aestwfi)</query>
    <earliest>$earliest$</earliest>
    <latest>$latest$</latest>
  </search>
  <search id="base_metrics">
    <query>`fuw_search_start_ims` sourcetype=atf-aes $imsid$ $tran$ $response_min$ $response_max$ | stats count, sum(aescpu), avg(aesxsrsp), values(aescorid), values(aestran), earliest(_time) as earliest, latest(_time) as latest</query>
    <earliest>$earliest$</earliest>
    <latest>$latest$</latest>
  </search>
  <search base="base_metrics">
    <query>eval startTime=strftime(earliest, "%x %H:%M:%S") 
| eval endTime=if(strftime(earliest, "%x")=strftime(latest, "%x"), strftime(latest, "%H:%M:%S"), strftime(latest, "%x %H:%M:%S")) | eval diff=(latest-earliest) | eval hours=floor(diff/3600) | eval minutes=floor((diff-(hours*3600))/60) | eval seconds=floor(diff-(hours*3600)-(minutes*60)) | eval duration=hours." hour".if(hours&gt;1,"s "," ").minutes." minute".if(minutes&gt;1,"s "," ").seconds." second".if(seconds&gt;1,"s","")</query>
    <progress>
      <condition match="'job.resultCount' &gt; 0">
        <set token="startTime">$result.startTime$</set>
        <set token="endTime">$result.endTime$</set>
        <set token="duration">$result.duration$</set>
        <set token="eventCount">$result.count$</set>
      </condition>
      <condition>
        <unset token="startTime"></unset>
        <unset token="endTime"></unset>
        <unset token="duration"></unset>
        <unset token="eventCount"></unset>
      </condition>
    </progress>
  </search>
  <fieldset submitButton="false">
    <input type="time" searchWhenChanged="true">
      <label>Time range</label>
      <default>
        <earliest>0</earliest>
        <latest></latest>
      </default>
    </input>
    <input type="multiselect" token="imsid" searchWhenChanged="true">
      <label>IMS subsystem</label>
      <default>*</default>
      <choice value="*">Any</choice>
      <search base="base_metrics">
        <query>fields values(aescorid) | rename values(aescorid) as value | mvexpand value | eval label=value</query>
      </search>
      <fieldForLabel>label</fieldForLabel>
      <fieldForValue>value</fieldForValue>
      <prefix>(</prefix>
      <suffix>)</suffix>
      <valuePrefix>aescorid=</valuePrefix>
      <delimiter> OR </delimiter>
    </input>
    <input type="multiselect" token="tran" searchWhenChanged="true">
      <label>IMS transaction code</label>
      <default>*</default>
      <choice value="*">Any</choice>
      <search base="base_metrics">
        <query>fields values(aestran) | rename values(aestran) as value | mvexpand value | eval label=value</query>
      </search>
      <fieldForLabel>label</fieldForLabel>
      <fieldForValue>value</fieldForValue>
      <prefix>(</prefix>
      <suffix>)</suffix>
      <valuePrefix>aestran=</valuePrefix>
      <delimiter> OR </delimiter>
    </input>
    <input type="text" token="response_min" searchWhenChanged="true">
      <label>Response time (min)</label>
      <prefix>aesxsrsp&gt;=</prefix>
      <default>0</default>
    </input>
    <input type="text" token="response_max" searchWhenChanged="true">
      <label>Response time (max)</label>
      <prefix>aesxsrsp&lt;=</prefix>
      <default></default>
    </input>
    <html depends="$eventCount$,$duration$,$startTime$,$endTime$">
      $eventCount$ events spanning $duration$ ($startTime$ to $endTime$)
    </html>
  </fieldset>
  <row>
    <panel>
      <title>Top 10 IMS subsystems</title>
      <table>
        <search>
          <query>`fuw_search_start_ims` sourcetype=atf-aes $imsid$ $tran$ $response_min$ $response_max$ | stats sum(aesxsrsp) as aesxsrsp by aescorid | sort -aesxsrsp | head 10 | eventstats sum(aesxsrsp) as total | eval percent=100*aesxsrsp/total | fields - total | rename aesxsrsp as "Total response time", percent as "% of top 10", aescorid as "IMS subsystem"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format field="Total response time" type="sparkline">
          <option name="lineColor">#5379af</option>
          <option name="fillColor">#CCDDFF</option>
          <option name="lineWidth">1</option>
          <option name="maxSpotColor">#A2FFA2</option>
          <option name="spotRadius">3</option>
          <option name="height">25px</option>
        </format>
        <format field="Number of transactions" type="sparkline">
